@model IEnumerable<BrakeBillCourseSchema.Models.Teacher>
@{
    ViewBag.Title = "Teachers";
}
<h2>Teachers</h2>
<div id="TeacherCreate">
    @Html.Partial("_TeacherCreate")
</div>
<ul class="row">
    <li class="col-md-3">
        @Html.DisplayNameFor(model => model.Firstname)
    </li>
    <li class="col-md-3">
        @Html.DisplayNameFor(model => model.Lastname)
    </li>
    <li class="col-md-6"></li>
    <li class="rowbreak"></li>
</ul>
@foreach (var item in Model)
{
    @Html.Partial("_Teacherlist","Teacher")
}